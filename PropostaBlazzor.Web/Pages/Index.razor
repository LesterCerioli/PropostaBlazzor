@page "/"

<div class="text-center">
    <form class="form-signin">
        <h1 class="h4 mb-2 login">Login de Acesso</h1>

        <label for="inputEmail" class="sr-only">Email</label>
        <input type="email" id="inputEmail"
               name="email" placeholder="Informe o seu Email"
               class="form-control mb-2" />

        <label for="inputSenha" class="sr-only">Senha</label>
        <input type="password" id="inputSenha"
               name="senha" placeholder="Senha"
               class="form-control mb-2" />


        <button class="btn btn-primary btn-block mb-3" type="submit" (click)="entrar()" [disabled]="ativar_spinner">
            <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"</span>
            Entrar
        </button>

        <img src="data:image/png;base64," />


    </form>
</div>